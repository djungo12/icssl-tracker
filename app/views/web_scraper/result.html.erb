<h1>NY Shellfish Shippers</h1>
<h5>CERTIFICATES EXPIRE DECEMBER 31, 2017</h5>
<div>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>City</th>
        <th>State</th> 
        <th>Cert. #</th>
        <th>Type</th>
      </tr>
    </thead>
    <tbody>    
      <tr>
        <% count = 1 %>
        <% @rows.each do |row| %>
        <td><%= "#{row.values[0]}" %></td>
        <td><%= "#{row.values[1]}" %></td>
        <td><%= "#{row.values[2]}" %></td> 
        <td><%= "#{row.values[3]}" %></td> 
        <td><%= "#{row.values[4]}" %></td>
        <% count +=1 %>
      </tr>
    <%  end %>
    </tbody>
    <p>There are <%= "#{count}" %> dealers total.</p>

  </table>
</div>
